doctype html
html(
  lang='ko'
)

  head
    meta(charset="UTF-8")
    meta(
      name="viewport",
      content="width=device-width, initial-scale=1",
    )
    title folder

body
  main
    h2 folderName
    table(border="0")
      tbody
        each fileName, index in files
          tr
            td= index
            td= fileName
  
